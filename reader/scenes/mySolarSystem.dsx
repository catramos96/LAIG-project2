        </component>
		
		<!-- Boards -->
	
		<component id="Boards">																														
        
            <transformation>
				<rotate axis="y" angle="-90"/>
            	<rotate axis="x" angle="-90"/>
            </transformation>
       
			<materials>
				<material id="inherit" />
            </materials>
                         
			<texture id="none" />

			<children>
            	<componentref id="Board1"/>
				<componentref id="Board2"/>
				<componentref id="Board3"/>
            </children>
            
        </component>
	
		<component id="Board1">																														
        
            <transformation>
            	<scale x="7" y="7" z="7" />
